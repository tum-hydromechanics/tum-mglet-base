{
    "time": {
        "mtstep": 100,
        "dt": 0.1,
        "tend": 10.0,
        "itinfo": 1,
        "itcheck": 10
    },
    "io": {
        "grids": "particle_grids.h5",
        "infile": "fields.h5",
        "outfile": "fields.h5"
    },
    "ib": {
        "type": "noib"
    },
    "flow": {
        "tu_level": 0.0,
    	"solve": true,
        "gmol": 0.01,
        "rho": 1.0,
        "uinf": ["var a := 1.0; a", "var b := 0; b", "var c := 0.0; c"],
        "gradp": [0.0, 0.0, 0.0],
        "pressuresolver": {
            "type": "sip",
            "epcorr": 0.000768,
            "nouter_min": 2,
            "nouter": 9,
            "ninner": 5,
            "omega": 1.1,
            "loglevel": 3
        },
        "lesmodel": {
            "model": "none"
        }
    },
    "probes": {
        "itsamp": 8,
        "tstart": 0.0,
        "arrays": [
            {
                "name": "the_probe",
                "variables": ["P"],
                "positions": [
                    [ 0.5, 0.5, 0.5 ]
                ]
            }
        ]
    },
    "snapshots": {
        "itsamp": 10,
        "tstart": 0.0,
        "fields": ["U", "V", "W", "P", "NPC"]
    },
    "particles": {
        "dread_part_h5": false,
        "dread_part_dict": true,
        "dread_obst_dict": false,
        "dwrite_part_h5": false,
        "dwrite_npc": true,
        "terminal": "verbose",
        "snapshot_step": 1,
       	"snapshot_npart": 1,
        "list_length": 1,
        "dinterp": true,
        "rk_method": "euler",
        "D":[0.0, 0.0, 0.0],
        "dgridstat": true,
	    "rt_ittot_start": 0,
        "tdx_step_range": [0, 10],
        "tdy_step_range": [0, 0],
        "avu_step_range": [-10, 10],
        "slice_direction": "X",
        "nslice_levels": 1,
        "nslices": [10],
        "slice_levels": [1.0]
    }
}
