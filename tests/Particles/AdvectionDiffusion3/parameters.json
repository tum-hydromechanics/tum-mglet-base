{
    "time": {
        "mtstep": 4000,
        "dt": 0.01,
        "tend": 40.0,
        "itinfo": 10,
        "itcheck": 10
    },
    "io": {
        "grids": "particle_grids.h5",
        "infile": "fields.h5",
        "outfile": "fields.h5"
    },
    "ib": {
        "type": "noib"
    },
    "flow": {
        "tu_level": 0.0,
    	"solve": false,
        "gmol": 0.01,
        "rho": 1.0,
        "uinf": ["var a := -1.0; a*y", "var b := 0; b*x", "var c := 0; c*x"],
        "gradp": [0.0, 0.0, 0.0],
        "pressuresolver": {
            "type": "sip",
            "epcorr": 0.000768,
            "nouter_min": 2,
            "nouter": 9,
            "ninner": 5,
            "omega": 1.1,
            "loglevel": 3
        },
        "lesmodel": {
            "model": "none"
        }
    },
    "particles": {
        "dread_part_dict": true,
        "terminal": "normal",
        "snapshot_step": 20,
        "dinterp": true,
        "rk_method": "williamson",
        "dturb_diff": false,
        "random_walk_mode": "gaussian2",
        "truncation_limit": 2.0,
        "D":[0.000, 0.002, 0.000],
        "dgridstat": true,
        "slice_direction": "X",
        "nslice_levels": 1,
        "nslices": [100],
        "slice_levels": [1.0]
    }
}
